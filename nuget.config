<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <!-- Always start clean so you control exactly which sources are used -->
    <packageSources>
        <clear />
        <!-- Public source -->
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
        <!-- Optional local dev feed (relative path) -->
        <add key="local-dev" value=".nuget/local" />
    </packageSources>
    <!-- NO packageSourceMapping - keeps graceful fallback behavior -->
    <!-- If local-dev is missing, NuGet automatically uses nuget.org -->
</configuration>
